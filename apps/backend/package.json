{
  "name": "recipt-backend",
  "version": "1.0.0",
  "description": "광남동성당 청소년위원회 예결산 관리 시스템 - 백엔드 API",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --testPathIgnorePatterns=.*\\.e2e-spec\\.ts$",
    "test:integration": "jest --testPathPattern=.*\\.integration-spec\\.ts$",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm migration:revert -d src/database/data-source.ts",
    "schema:drop": "typeorm schema:drop -d src/database/data-source.ts",
    "schema:sync": "typeorm schema:sync -d src/database/data-source.ts",
    "seed": "ts-node --esm -r tsconfig-paths/register src/database/seeds/index.ts",
    "seed:organizations": "ts-node --esm -r tsconfig-paths/register src/database/seeds/index.ts --specific 조직",
    "seed:users": "ts-node --esm -r tsconfig-paths/register src/database/seeds/index.ts --specific 사용자",
    "seed:relations": "ts-node --esm -r tsconfig-paths/register src/database/seeds/index.ts --specific 관계",
    "type-check": "tsc --noEmit",
    "db:init": "ts-node --esm -r tsconfig-paths/register src/database/scripts/database-initializer.ts",
    "db:init:clean": "ts-node --esm -r tsconfig-paths/register src/database/scripts/database-initializer.ts --drop-schema --yes",
    "db:backup": "ts-node --esm -r tsconfig-paths/register src/database/scripts/backup-restore.ts backup",
    "db:backup:schema": "ts-node --esm -r tsconfig-paths/register src/database/scripts/backup-restore.ts backup schema",
    "db:backup:data": "ts-node --esm -r tsconfig-paths/register src/database/scripts/backup-restore.ts backup data",
    "db:restore": "ts-node --esm -r tsconfig-paths/register src/database/scripts/backup-restore.ts restore",
    "db:list-backups": "ts-node --esm -r tsconfig-paths/register src/database/scripts/backup-restore.ts list",
    "db:status": "ts-node --esm -r tsconfig-paths/register src/database/scripts/database-utils.ts status",
    "db:performance": "ts-node --esm -r tsconfig-paths/register src/database/scripts/database-utils.ts performance",
    "db:cleanup": "ts-node --esm -r tsconfig-paths/register src/database/scripts/database-utils.ts cleanup",
    "db:reset-dev": "ts-node --esm -r tsconfig-paths/register src/database/scripts/database-utils.ts reset-dev",
    "db:validate": "ts-node --esm -r tsconfig-paths/register src/database/scripts/database-utils.ts validate",
    "bulk-data": "ts-node --esm -r tsconfig-paths/register src/database/scripts/bulk-data-generator.ts",
    "bulk-data:small": "ts-node --esm -r tsconfig-paths/register src/database/scripts/bulk-data-generator.ts --users=100 --events=50 --receipts=500",
    "bulk-data:large": "ts-node --esm -r tsconfig-paths/register src/database/scripts/bulk-data-generator.ts --users=5000 --events=1000 --receipts=25000",
    "db:optimize-indexes": "ts-node --esm -r tsconfig-paths/register src/database/scripts/index-optimization.ts",
    "db:analyze-performance": "ts-node --esm -r tsconfig-paths/register src/database/scripts/query-analyzer.ts",
    "db:analyze-slow-queries": "ts-node --esm -r tsconfig-paths/register src/database/scripts/query-analyzer.ts --slow-queries",
    "db:analyze-indexes": "ts-node --esm -r tsconfig-paths/register src/database/scripts/query-analyzer.ts --index-efficiency",
    "db:analyze-tables": "ts-node --esm -r tsconfig-paths/register src/database/scripts/query-analyzer.ts --table-stats"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "typeorm": "^0.3.17",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "cache-manager": "^5.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "uuid": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.0",
    "axios": "^1.4.0",
    "moment": "^2.29.4",
    "rxjs": "^7.8.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@faker-js/faker": "^8.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/bcrypt": "^5.0.0",
    "@types/uuid": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/recipt-management.git"
  },
  "author": "광남동성당 청소년위원회 개발팀",
  "license": "MIT"
}